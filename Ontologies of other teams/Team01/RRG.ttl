@prefix sswap: <http://sswapmeet.sswap.info/sswap/> .
@prefix cb: <http://example.org/cottage-booking#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/cottage-booking/getCottages/response>
    a sswap:Response ;
    sswap:resultOf <http://example.org/cottage-booking/getCottages/invoke> ;
    sswap:mapsTo [
        a cb:BookingResponse ;
        cb:bookingNumber "BKG123456" ;
        cb:bookerName "John Doe" ;
        cb:city "Jyvaskyla" ;
        cb:address "123 Lake Road, Jyvaskyla" ;
        cb:imageUrl "http://example.com/images/cottage1.jpg" ;
        cb:startDate "2024-12-15"^^xsd:date ;
        cb:endDate "2024-12-22"^^xsd:date ;
        cb:numberOfPeople 4 ;
        cb:numberOfBedrooms 2 ;
        cb:lakeDistance 100.0^^xsd:float ;
        cb:cityDistance 10.0^^xsd:float
    ] .
